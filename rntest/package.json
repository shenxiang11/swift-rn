{
  "name": "rntest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle:common": "react-native bundle --reset-cache --platform ios --dev false --entry-file ./common.js --bundle-output ~/Desktop/common.bundle.js --assets-dest ~/Desktop/common --config ./commonDepConfig.js",
    "bundle:home": "react-native bundle --reset-cache --platform ios --dev false --entry-file ./App.js --bundle-output ~/Desktop/home.bundle.js --assets-dest ~/Desktop --config ./bizConfig.js",
    "bundle:profile": "react-native bundle --reset-cache --platform ios --dev false --entry-file ./Profile.js --bundle-output ~/Desktop/profile.bundle.js --assets-dest ~/Desktop --config ./bizConfig.js"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.67.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
